datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                  Int       @id @default(autoincrement())
  username            String    @unique
  firstName           String
  lastName            String
  email               String    @unique
  password            String
  walletAddress       String?   
  createdAt           DateTime  @default(now())
}
